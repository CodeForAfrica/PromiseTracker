{
	"presets": [
		"next/babel"
	],
	"plugins": [
		[
			"transform-imports",
			{
				"material-ui/core": {
					"transform": "material-ui/core/${member}",
					"preventFullImport": true
				},
				"material-ui/icons": {
					"transform": "material-ui/icons/${member}",
					"preventFullImport": true
				},
				"hurumap-ui/core": {
					"transform": "hurumap-ui/core/${member}",
					"preventFullImport": true
				},
				"hurumap-ui/charts": {
					"transform": "hurumap-ui/charts/${member}",
					"preventFullImport": true
				}
			}
		],
		[
			"module-resolver",
			{
				"root": [
					"./"
				],
				"alias": {
					"@/promisetracker": "./src"
				}
			}
		]
	]
}

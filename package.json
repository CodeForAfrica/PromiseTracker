    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/utils": "^4.10.2",
    "@sentry/nextjs": "^6.17.4",
    "@svgr/webpack": "^6.2.1",
    "apollo-cache": "^1.3.2",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.12",
    "apollo-link-http": "^1.5.17",
    "apollo-utilities": "^1.3.4",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "form-data": "^4.0.0",
    "formik": "^2.2.9",
    "formik-material-ui": "^3.0.1",
    "graphql": "^14.7.0",
    "graphql-tag": "^2.12.6",
    "jwt-decode": "^3.1.2",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.21",
    "next": "^12.1.0",
    "next-auth": "^4.3.2",
    "next-seo": "^4.23.0",
    "papaparse": "^5.3.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-apollo": "^3.1.5",
    "react-apollo-hooks": "^0.5.0",
    "react-dom": "^17.0.2",
    "react-share": "^4.4.0",
    "react-swipeable-views": "^0.14.0",
    "reading-time": "^1.2.1",
    "simplebar-react": "^2.3.6",
    "victory": "^36.2.1",
    "webpack": "^5.68.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/eslint-parser": "^7.17.0",
    "@material-ui/codemod": "^4.5.0",
    "@types/react": "^17.0.39",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-markdown": "^2.0.1",
    "eslint-plugin-module-resolver": "^1.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jscodeshift": "^0.13.1",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "svg-url-loader": "^7.1.1"
  },
  "scripts": {
    "build": "next build",
    "dev": "yarn dev:next",
    "dev:next": "next dev",
    "export": "yarn run build && next export",
    "format": "yarn prettier --write --ignore-path './.gitignore' '**/*.+(json|md|yml)' '.'",
    "format-staged": "yarn prettier --write '**/*.+(json|md|yml)'",
    "codemod": "yarn codemod-staged-optimal-imports './src' && yarn codemod-staged-top-level-imports './src'",
    "codemod-staged-optimal-imports": "yarn jscodeshift --transform=node_modules/@material-ui/codemod/lib/v4.0.0/optimal-imports.js",
    "codemod-staged-top-level-imports": "yarn jscodeshift --transform=node_modules/@material-ui/codemod/lib/v4.0.0/top-level-imports.js",
    "lint": "yarn eslint --fix --ignore-path './.gitignore' './'",
    "lint-staged": "yarn eslint --fix --ext '.js,.json,.md'",
    "start": "next start",
    "prepare": "husky install"
  },
  "browserslist": [
    "defaults"
  ],
  "lint-staged": {
    "*.js": [
      "yarn codemod-staged-optimal-imports",
      "yarn codemod-staged-top-level-imports",
      "yarn lint-staged"
    ],
    "*.{json,md}": [
      "yarn lint-staged",
      "yarn format-staged"
    ],
    "*.yml": [
      "yarn format-staged"
    ]
  },
  "engines": {
    "node": ">=12.18.0 <15.x",
    "yarn": ">=1.19.2 <2.x"
  }
}

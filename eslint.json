{
  "root": true,
  "parser": "babel-eslint",
  "extends": [
    "plugin:prettier/recommended",
    "prettier/react"
  ],
  "env": {
    "browser": true
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "rules": {
    "module-resolver/use-alias": 2,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js"
        ]
      }
    ],
    "react/jsx-props-no-spreading": 0
  }
}
